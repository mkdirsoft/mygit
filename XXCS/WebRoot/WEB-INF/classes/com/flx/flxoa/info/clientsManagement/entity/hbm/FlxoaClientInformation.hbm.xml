<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.clientsManagement.entity">
	<class
		name="FlxoaClientInformation"
		table="flxoa_client_information"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ClientName"
			column="client_name"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="ClientRank"
			column="client_rank"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ClientBusiness"
			column="client_business"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ClientTrade"
			column="client_trade"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ClientAddress"
			column="client_address"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="ClientSalesman"
			column="client_salesman"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="ClientSiteurl"
			column="client_siteURL"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="ClientPhone"
			column="client_phone"
			type="string"
			not-null="true"
			length="11"
		/>
		<property
			name="ClientMail"
			column="client_mail"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="ClientCellphone"
			column="client_cellphone"
			type="string"
			not-null="true"
			length="11"
		/>
		<property
			name="ClientPostcode"
			column="client_postcode"
			type="string"
			not-null="true"
			length="6"
		/>
		<property
			name="ClientFax"
			column="client_fax"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="Comments"
			column="comments"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="11"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>