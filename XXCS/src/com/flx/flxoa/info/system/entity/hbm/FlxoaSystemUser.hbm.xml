<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.system.entity">
	<class
		name="FlxoaSystemUser"
		table="flxoa_system_user"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="UserName"
			column="user_name"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="Password"
			column="password"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="RealName"
			column="real_name"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="WeixinId"
			column="weixin_id"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="CompanyId"
			column="company_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DepartmentId"
			column="department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="Gender"
			column="gender"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="Types"
			column="types"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="Birthday"
			column="birthday"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="EnterUnitTime"
			column="enter_unit_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="IdentificationNumber"
			column="identification_number"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="UnitName"
			column="unit_name"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="EmploymentTime"
			column="employment_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ValidDocumentCategory"
			column="valid_document_category"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="HighestDegreeEducation"
			column="highest_degree_education"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CardId"
			column="card_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="IsPhoneSign"
			column="is_phone_sign"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="Position"
			column="position"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="WorkPlace"
			column="work_place"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="PhoneId"
			column="phone_id"
			type="string"
			not-null="true"
			length="200"
		/>		
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>