<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.projectmanagement.entity">
	<class
		name="FlxoaProjectInformation"
		table="flxoa_project_information"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>


		<property
			name="ProjNumber"
			column="proj_number"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="ProjName"
			column="proj_name"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="ProjAttribute"
			column="proj_attribute"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="BusClassification"
			column="bus_classification"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="DesignCompany"
			column="design_company"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ProjState"
			column="proj_state"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ProjStartTime"
			column="proj_start_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ProjEndTime"
			column="proj_end_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Signatory"
			column="signatory"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="SignatoryName"
			column="signatory_name"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="SignatoryTelephone"
			column="signatory_telephone"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="ProjStage"
			column="proj_stage"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="WhetherFollow"
			column="whether_follow"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ClientName"
			column="client_name"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="PredictContractMoney"
			column="predict_contract_money"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="PredictBidsTime"
			column="predict_bids_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ProjectLeader"
			column="project_leader"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>