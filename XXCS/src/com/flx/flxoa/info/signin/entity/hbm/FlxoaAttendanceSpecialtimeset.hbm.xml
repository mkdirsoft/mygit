<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.signin.entity">
	<class
		name="FlxoaAttendanceSpecialtimeset"
		table="flxoa_attendance_specialtimeset"
	>
		<meta attribute="sync-DAO">false</meta>

		<!-- <meta attribute="sync-DAO">false</meta> -->

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="SpecialStartDate"
			column="special_start_date"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="SpecialEndDate"
			column="special_end_date"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Gotoworktime"
			column="gotoworktime"
			type="string"
			not-null="true"
			length="25"
		/>
		<property
			name="Afterworktime"
			column="afterworktime"
			type="string"
			not-null="true"
			length="25"
		/>
		<property
			name="SpecialGroup"
			column="special_group"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="SpecialGroupDetails"
			column="special_group_details"
			type="string"
			not-null="true"
			length="400"
		/>
		<property
			name="Flag"
			column="flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>