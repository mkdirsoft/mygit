			name="SignEarliestTime"
			column="sign_earliest_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="SignEarliestType"
			column="sign_earliest_type"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="SignEarliestComments"
			column="sign_earliest_comments"
			type="string"
			not-null="true"
			length="500"
		/>
		<property
			name="SignEarliestAddress"
			column="sign_earliest_address"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="SignLatestTime"
			column="sign_latest_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="SignLatestType"
			column="sign_latest_type"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="SignLatestComments"
			column="sign_latest_comments"
			type="string"
			not-null="true"
			length="500"
		/>
		<property
			name="SignLatestAdress"
			column="sign_latest_adress"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="LeaderComments"
			column="leader_comments"
			type="string"
			not-null="true"
			length="500"
		/>
		<property
			name="CommetsTime"
			column="commets_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>