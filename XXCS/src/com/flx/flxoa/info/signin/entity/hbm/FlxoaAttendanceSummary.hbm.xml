			<generator class="identity"/>
		</id>

		<property
			name="SignStatus"
			column="sign_status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="SignData"
			column="sign_data"
			type="integer"
			not-null="true"
			length="10"
