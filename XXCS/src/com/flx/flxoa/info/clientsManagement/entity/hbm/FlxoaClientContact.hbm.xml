<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.clientsManagement.entity">
	<class
		name="FlxoaClientContact"
		table="flxoa_client_contact"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ClientId"
			column="client_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ContactName"
			column="contact_name"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="ContactGender"
			column="contact_gender"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ContactDuty"
			column="contact_duty"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="ContactPhone"
			column="contact_phone"
			type="string"
			not-null="true"
			length="11"
		/>
		<property
			name="ContactMail"
			column="contact_mail"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="11"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>