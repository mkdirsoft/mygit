<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.approve.entity">
	<class
		name="FlxoaApproveForm"
		table="flxoa_approve_form"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="TemplateId"
			column="template_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="SubmitStatus"
			column="submit_status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="SubmitTime"
			column="submit_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="WorkflowId"
			column="workflow_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CheckUserIds"
			column="check_user_ids"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="NowFormStatus"
			column="now_form_status"
			type="string"
			not-null="true"
			length="50"
		/>		
		<property
			name="IsRead"
			column="is_read"
			type="string"
			not-null="true"
			length="2"
		/>				
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>