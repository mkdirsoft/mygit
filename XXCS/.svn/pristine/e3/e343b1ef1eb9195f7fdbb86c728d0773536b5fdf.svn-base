package com.flx.flxoa.info.system.entity;

import java.util.ArrayList;
import java.util.List;

/**
 * 类名称：FunctionJsonBean.java
 * 类描述：
 * 创建时间：2018-3-19, 下午7:14:07
 * 
 * @version 1.0 
 * @since JDK版本 1.7
 * @author 刘凯 
 */
public class FunctionJsonBean {

	private String id;//id
	private String name;//功能名称
	private String url;//功能url
	private String pid;//父级id
	private String isActive;//是否点击
	private List<FunctionJsonBean> subMenu = new ArrayList<FunctionJsonBean>();//子级菜单
	
	public String getPid() {
		return pid;
	}
	public void setPid(String pid) {
		this.pid = pid;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	public String getIsActive() {
		return isActive;
	}
	public void setIsActive(String isActive) {
		this.isActive = isActive;
	}
	public List<FunctionJsonBean> getSubMenu() {
		return subMenu;
	}
	public void setSubMenu(List<FunctionJsonBean> subMenu) {
		this.subMenu = subMenu;
	}
	
}
