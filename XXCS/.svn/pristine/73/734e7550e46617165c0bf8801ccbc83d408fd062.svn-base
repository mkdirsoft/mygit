<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.signin.entity">
	<class
		name="FlxoaAttendanceSigndetails"
		table="flxoa_attendance_signdetails"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="SignDate"
			column="sign_date"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="SignTime"
			column="sign_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="SignLocale"
			column="sign_locale"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="SignLongitude"
			column="sign_longitude"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="SignLatitude"
			column="sign_latitude"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="Commens"
			column="commens"
			type="string"
			not-null="true"
			length="300"
		/>
		<property
			name="SignType"
			column="sign_type"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>