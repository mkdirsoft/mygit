<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.clientsManagement.entity">
	<class
		name="FlxoaClientCommunication"
		table="flxoa_client_communication"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ProjId"
			column="proj_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ContactId"
			column="contact_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CommStartTime"
			column="comm_start_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CommEndTime"
			column="comm_end_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CommPattern"
			column="comm_pattern"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CommMainContent"
			column="comm_main_content"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="11"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>