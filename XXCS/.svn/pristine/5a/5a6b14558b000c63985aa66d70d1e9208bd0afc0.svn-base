<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.system.entity">
	<class
		name="FlxoaSystemFuncation"
		table="flxoa_system_funcation"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="ParentId"
			column="parent_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Url"
			column="url"
			type="string"
			not-null="true"
			length="500"
		/>
		<property
			name="IsMenu"
			column="is_menu"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ButtonId"
			column="button_id"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="FunctionSort"
			column="function_sort"
			type="integer"
			not-null="true"
			length="10"
		/>		
		<property
			name="Description"
			column="description"
			type="string"
			not-null="true"
			length="400"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>