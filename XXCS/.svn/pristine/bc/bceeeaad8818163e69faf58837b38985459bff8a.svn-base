<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.contractmanagement.entity">
	<class
		name="FlxoaContractLoanreturn"
		table="flxoa_contract_loanreturn"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ContractId"
			column="contract_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="FormId"
			column="form_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="LoanType"
			column="loan_type"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="EstimateReturn"
			column="estimate_return"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="LoanTime"
			column="loan_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ReturnTime"
			column="return_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="LoanStatus"
			column="loan_status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="LoanNumber"
			column="loan_number"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="LoanScan"
			column="loan_scan"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="LoanPeople"
			column="loan_people"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ReturnPeople"
			column="return_people"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="true"
			length="400"
		/>


	</class>	
</hibernate-mapping>