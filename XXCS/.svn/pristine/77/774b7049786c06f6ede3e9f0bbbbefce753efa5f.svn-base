/*清除浮动：*/
.clearfix:after {
	content:".";
	display:block;
	height:0;
	clear:both;
	visibility:hidden;
}
.clearfix {
	*+height:1%;
}
/*字体 白色*/
.text-c-white-oy {
	color: #fff !important;
}
/*大屏xl 栅格 分为5格*/
@media (min-width: 1200px) {
	.col-xl-2dot4-oy {
		-webkit-box-flex: 0;
	    -ms-flex: 0 0 20%;
	    flex: 0 0 20%;
	    max-width: 20%;
	}
}
/*卡片 卡片头部&卡片尾部 新增小型样式*/
.card .card-header-small-oy {
	padding: 15px;
}
.card .card-header-small-oy .card-header-right {
    top: 5px;
}
.card .card-footer-small-oy {
	padding: 15px;
}




/*查询条件*/
.search-input-oy {
	margin-bottom: 20px;
}
.search-input-oy .form-control {
	padding: 8px 12px;
	height: 35px;
}
.search-input-oy select.form-control:not([size]):not([multiple]) {
    padding: 6px 12px 8px 12px;
    height: 35px;
}
.search-btn-oy {
	text-align: center;
}
.search-btn-oy button {
	margin-left: 15px;
	margin-right: 15px;
}
/*表格 右上方 批量操作按钮*/
.action-buttons-oy {
	text-align: right;
	margin-bottom: 10px;
}
/*弹出模态框 样式*/
.layui-layer-con-oy {
	padding: 20px;
}








/*登录页*/
.dl-wrapper {	
	position: relative;
}
.dl-row {
	border-bottom: 1px solid #dadada;
	padding-bottom: 10px;
	text-align: left;
}
.dl-row.col-4 {
	margin-right: 100px;
	padding-left: 0;
    padding-right: 0;
}
.dl-row.col-4 .checkbox-fade label {
    margin-bottom: 0;
}
.dl-input input {
	border: none;
	outline: none;
	background: none !important;
	width: 180px;
    height: 35px;
}
.dl-input input:-webkit-autofill { 
	-webkit-box-shadow: 0 0 0px 1000px #fff inset; 
	border: none; 
}
.dl-row-btn.col-4 {
	margin-right: 100px;
    padding-left: 0;
    padding-right: 0;
    margin-top: 50px;
}
.dl-row-btn.col-4 .checkbox-fade {
	width: 100%;
}
.dl-row-btn.col-4 .checkbox-fade button {
	width: 100%;
}
.dl-tab-tit {
	width: 567px;
    float: right;
    border-bottom: 1px solid #dadada;
    height: 79px;
    line-height: 79px;
}
.dl-tab-tit li {
	width: 50%;
	float: left;
	text-align: center;
}
.dl-tab-tit li a {
	color: #666 !important;
	font-size: 22px;
	cursor: pointer;
}
.dl-tab-tit li a:hover {
	color: #4099ff !important;
}
.dl-tab-tit li:first-child {
	background: url(../img/dl-tab-tit-line.png) no-repeat right center;
}
.dl-tab-tit li.selected a {
	color: #4099ff !important;
}
.dl-qrcode-btn {
	position: absolute;
	right: 2%;
	top:5%;
	cursor: pointer;
}
.dl-qrcode {
	text-align: center;	
	width: 567px;
	float: right;
}
.dl-qrcode img {
	width: 160px;
}
.dl-qrcode-tips {
	text-align: center;
}




