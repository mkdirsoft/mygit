/*项目管理页*/
.xmgl-multi-tab-header a span {
	display: inline !important;
}
.xmgl-multi-tab-tit li {
	float: left;
	width: 8%;
	margin-left: 1%;
	margin-right: 1%;
	text-align: center;
	border-radius: 5px;
	position: relative;
	cursor: pointer;
}
.xmgl-multi-tab-tit li i {
	display: block;
	margin-top: 20px;
	margin-bottom: 5px;
}
.xmgl-multi-tab-tit li span {
	display: block;
	color: #fff;
	font-size: 18px;
}
.xmgl-multi-tab-tit li a {
	display: block;
	color: #fff !important;
	font-size: 14px;
	margin-bottom: 8px;
}
.xmgl-multi-tab-tit li:nth-child(1) {
	background-color: #489dff;
}
.xmgl-multi-tab-tit li:nth-child(2) {
	background-color: #3cdabb;
}
.xmgl-multi-tab-tit li:nth-child(3) {
	background-color: #ffbe61;
}
.xmgl-multi-tab-tit li:nth-child(4) {
	background-color: #ff7d92;
}
.xmgl-multi-tab-tit li:nth-child(5) {
	background-color: #06dbf6;
}
.xmgl-multi-tab-tit li:nth-child(6) {
	background-color: #3cdabb;
}
.xmgl-multi-tab-tit li:nth-child(7) {
	background-color: #ffbc5c;
}
.xmgl-multi-tab-tit li:nth-child(8) {
	background-color: #ff7d92;
}
.xmgl-multi-tab-tit li:nth-child(9) {
	background-color: #489dff;
}
.xmgl-multi-tab-tit li:nth-child(10) {
	background-color: #fe8a7d;
}
.xmgl-multi-tab-tit li.selected em {
	position: absolute;
	top: -8px;
	right: -8px;
	display: block;
	width: 28px;
	height: 28px;
	background-image: url(../img/xmgl-tab-selected.png);
	background-repeat: no-repeat;
	background-position: center;
}
.xmgl-multi-tab-tit li i img {
	transform: scale(0.8,0.8);
}
.xmgl-multi-tab-tit li:hover i img {	
	transform: scale(1,1);
	transition: transform 0.5s;
}
.xmgl-multi-tab-tit li:hover {
	box-shadow: 0px 0px 10px 5px rgba(0, 0, 0, 0.1);
	transition: box-shadow 0.5s;
}




/*投标管理页-分步骤插件*/
.wizard>.steps {    
    background: url(../img/step_line.png) repeat-x left 12px;
}
.wizard>.steps>ul>li {
    width: 14.2857142857%;
    position: relative;
    text-align: center;
}
.wizard>.steps>ul>li>a {
	cursor: default !important;
    background: none !important;
} 
.wizard>.steps .number {    
    margin-left: 0;
    display: block;
    width: 30px;
    height: 30px;
    margin: auto;    
    text-align: center;
    line-height: 30px;
    border-radius: 50%;
    margin-bottom: 10px;
}
.wizard>.steps .title {
	font-size: 12px;
}
.wizard>.steps .date {
	display: block;
}
.wizard>.steps a, .wizard>.steps a:hover, .wizard>.steps a:active {  
	background: none;  
    padding: 0;
    margin: 0;
}
.wizard>.steps .current a, .wizard>.steps .current a:hover, .wizard>.steps .current a:active {    
    outline: 0;
    outline-offset: 0;
}
.wizard>.steps .current .number {       
    background: #4099ff;    
}
.wizard>.steps .current .title {
    color: #4099ff;
}
.wizard>.steps .current .date {
    color: #4099ff;
}
.wizard>.steps .done .number {       
    background: #8ec3ff;    
}
.wizard>.steps .done .title {
    color: #8ec3ff;    
}
.wizard>.steps .done .date {
    color: #8ec3ff;    
}
.wizard>.steps .disabled .number { 
	color: #fff;      
    background: #aaa;  
}
.wizard>.steps .disabled .title {
    color: #aaa;  
}
.wizard>.steps .disabled .date { 
    color: #aaa;  
}
.wizard>.content {
    border: none;    
    min-height: 13em;
    margin: 15px 0;   
    background: #f8f8f8; 
}
.wizard>.content>.body {   
    width: 100%;
}
.wizard>.actions a, .wizard>.actions a:hover, .wizard>.actions a:active {
    background: #4099ff;    
}
