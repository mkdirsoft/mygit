		java.lang.String name,
		java.lang.Integer parentId,
		java.lang.String url,
		java.lang.String isMenu,
		java.lang.String buttonId,
		java.lang.Integer functionSort,
		java.lang.String description,
		java.lang.Integer createTime,
		java.lang.Integer updateTime,
		java.lang.String deleteFlag,
		java.lang.Integer createUserId,
		java.lang.Integer createDepartmentId,
		java.lang.Integer updateUserId,
		java.lang.Integer updateDepartmentId) {

		super (
			id,
			name,
			parentId,
			url,
			isMenu,
			buttonId,
			functionSort,
			description,
			createTime,
			updateTime,
			deleteFlag,
			createUserId,
			createDepartmentId,
			updateUserId,
			updateDepartmentId);
	}

/*[CONSTRUCTOR MARKER END]*/
	//不跳转url
	private String urlBack;
	//是否选中
	private boolean checked;
	
	public boolean isChecked() {
		return checked;
	}

