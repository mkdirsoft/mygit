<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.cashcollection.entity">
	<class
		name="FlxoaCashcollectionRecord"
		table="flxoa_cashcollection_record"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="FundSource"
			column="fund_source"
			type="string"
			not-null="true"
			length="300"
		/>
		<property
			name="AccountNumber"
			column="account_number"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="CaroOrgName"
			column="caro_org_name"
			type="string"
			not-null="true"
			length="600"
		/>
		<property
			name="CaroMoney"
			column="caro_money"
			type="big_decimal"
			not-null="true"
			length="15"
		/>
		<property
			name="CaroUse"
			column="caro_use"
			type="string"
			not-null="true"
		/>
		<property
			name="CaroType"
			column="caro_type"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ProjectId"
			column="project_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ClaimId"
			column="claim_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ClaimTime"
			column="claim_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="AffirmId"
			column="affirm_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="AffirmTime"
			column="affirm_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="Pid"
			column="Pid"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Isshow"
			column="isshow"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
	</class>	
</hibernate-mapping>