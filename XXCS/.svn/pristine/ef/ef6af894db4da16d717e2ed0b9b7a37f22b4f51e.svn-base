<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.flx.flxoa.info.approve.entity">
	<class
		name="FlxoaApproveFormlog"
		table="flxoa_approve_formlog"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="NowStatus"
			column="now_status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ApproveUser"
			column="approve_user"
			type="string"
			not-null="true"
			length="45"
		/>
		<property
			name="ApproveTime"
			column="approve_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="NextStatus"
			column="next_status"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ApproveRemark"
			column="approve_remark"
			type="string"
			not-null="true"
			length="400"
		/>
		<property
			name="ApproveIdea"
			column="approve_idea"
			type="string"
			not-null="true"
			length="30"
		/>
		<property
			name="FormId"
			column="form_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateTime"
			column="update_time"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DeleteFlag"
			column="delete_flag"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CreateUserId"
			column="create_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CreateDepartmentId"
			column="create_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateUserId"
			column="update_user_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="UpdateDepartmentId"
			column="update_department_id"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>